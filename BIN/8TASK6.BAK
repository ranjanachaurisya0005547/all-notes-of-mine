//WAP to print the sum of digit of given number
#include<stdio.h>
#include<conio.h>
void main()
{
  long int num,i,rem,temp,sum=0;
  clrscr();
  printf("Enter Any Integer Value:");
  scanf("%ld",&num);

  temp=num;
  for(i=num;i>0;num=num/10)
  {
     rem=num%10;
     sum=sum+rem;
  }
  printf("%ld digit Addition=%ld",temp,sum);

  getch();
}