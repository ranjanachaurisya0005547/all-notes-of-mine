//WAP to print sum of digit of three digit numbers
#include<stdio.h>
#include<conio.h>
void main()
{
   long int num,sum=0,rem,temp=0;
   clrscr();
   printf("Enter Any Three Digit Value:");
   scanf("%ld",&num);
   temp=num;
   while(temp>0)
   {
      rem=temp%10;);
      sum=sum+rem;
      temp=temp/10;
   }
   printf("%ld Digit Addition is=%ld",num,sum);

   getch();
}