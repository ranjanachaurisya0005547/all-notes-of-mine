//WAP to read 5 numbers and sum of all odd values between them
#include<stdio.h>
#include<conio.n>
void main()
{
   int num,sum=0,i;
   clrscr();
   printf("Enter any five integer values:");

   for(i=1;i<=5;i++)
   {
      scanf("%d",&num);
      if(num%2!=0)
	sum=sum+num;

   }
   printf("Sum of odd number=%d",sum);
   getch();
}